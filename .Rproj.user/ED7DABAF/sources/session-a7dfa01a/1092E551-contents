git remote add origin https://github.com/iramler/imdb_tv_shows


# Rename master to main locally
git branch -m master main

# Push main branch to GitHub
git push -u origin main

# Delete master branch from GitHub
git push origin --delete master


usethis::use_github_action(url="https://github.com/posit-dev/r-shinylive/blob/actions-v1/examples/deploy-app.yaml")